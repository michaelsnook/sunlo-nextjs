{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "app/*": [
        "./app/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "lib/*": [
        "./lib/*"
      ],
      "styles/*": [
        "./styles/*"
      ],
      "types/*": [
        "./types/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": false
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
  ],
  "exclude": [
    "node_modules",
    "src-tauri",
    "out"
  ]
}
